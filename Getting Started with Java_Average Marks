import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
	Scanner s=new Scanner(System.in);
        String st=s.next();
        char ch=st.charAt(0);
        int m1=s.nextInt();
         int m2=s.nextInt();
         int m3=s.nextInt();
        System.out.println(ch);
          System.out.println((m1+m2+m3)/3);
    }
}
        
