import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int N = sc.nextInt();
    for(int i=1;i<=N;i++)
    {   int space=1;
        while(space<=N-i)
        {
            System.out.print(" ");
            space=space+1;
        }
     for(int j=1;j<=i;j++)
     {
         System.out.print(j);
     }
     System.out.println();
    }
        
        
    
	}

}
