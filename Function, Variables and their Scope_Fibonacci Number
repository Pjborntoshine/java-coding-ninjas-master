import java.util.Scanner;
public class Solution {
    static  boolean isPerfectSquare(int x) 
    { 
        int s = (int) Math.sqrt(x); 
        return (s*s == x); 
    } 
      
public static boolean checkMember(int n){
 	
 return isPerfectSquare(5*n*n + 4) || 
               isPerfectSquare(5*n*n - 4); 
	}
	}
