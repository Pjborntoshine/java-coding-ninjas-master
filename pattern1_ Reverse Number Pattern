import java.util.Scanner;
public class Solution {
public static void main(String[] args) {
	 Scanner s= new Scanner(System.in);
        int n = s.nextInt();
        int i=1;
        while(i<=n)
        {   int j=1;
            int sp=i;
         while(j<=i)
         {
             System.out.print(sp);
             sp--;
             j++;
         }
            i++;
            System.out.println();
        }
		

		
	}

}
